@model Model.EF.CW_Booking
@{
    ViewBag.Title = "Thông tin khách hàng Book chuyến bay";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}



<section class="content-header">
    <h1>
        Quản lý Booking
    </h1>
    <ol class="breadcrumb">
        <li><a href="/admin"><i class="fa fa-dashboard"></i> Trang chủ</a></li>
        <li><a href="#">Booking</a></li>

    </ol>
</section>
<!-- Main content -->
<section class="content">
    <!-- Default box -->
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">
            Thông tin khách hàng book chuyến bay
            </h3>
          

        </div><!-- /.box-header -->
        <div class="box-body">
            <section class="invoice">
                <div class="row">
                    <div class="col-xs-12">
                        <h2 class="page-header">
                            Khách hàng: @Model.FullName
                        </h2>
                        <h4>Email: @Model.Email</h4>
                        <h4>Phone: @Model.Phone</h4>
                    </div>
                    <!-- /.col -->
                </div>
                <!-- title row -->
                @*<div class="row">
                    <div class="col-xs-12">
                        <h2 class="page-header">
                            <i class="fa fa-globe"></i> AdminLTE, Inc.
                            <small class="pull-right">Date: 2/10/2014</small>
                        </h2>
                    </div>
        
                </div>*@
                <!-- info row -->
                <div class="row invoice-info">
                    <div class="col-sm-4 invoice-col">
                        Message
                        <address>
                         @Model.Message
                        </address>
                    </div>
                    <!-- /.col -->
                    <div class="col-sm-4 invoice-col">
                        InfoFly
                        <address>
                          @Model.InfoFly
                        </address>
                    </div>
                    <!-- /.col -->
                    <div class="col-sm-4 invoice-col">
                        InfoCus
                        <address>
                            @Model.InfoCus
                        </address>
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
                <div class="row">
                    <!-- accepted payments column -->
                    <div class="col-xs-6">
                        <p class="lead">Payment:</p>
                    </div>
                    <!-- /.col -->
                    <div class="col-xs-6">
                        <p class="lead">@Model.Payment</p>

                    </div>
                    <!-- /.col -->
                </div>

                <!-- /.row -->
                <div class="row">
                    <!-- accepted payments column -->
                    <div class="col-xs-6">
                        <p class="lead">Payment Date:</p>
                    </div>
                    <!-- /.col -->
                    <div class="col-xs-6">
                        <p class="lead">@Model.PayDate</p>

                    </div>
                    <!-- /.col -->
                </div>
                <div class="row">
                    <!-- accepted payments column -->
                    <div class="col-xs-6">
                        <p class="lead">DateCreated:</p>
                    </div>
                    <!-- /.col -->
                    <div class="col-xs-6">
                        <p class="lead">@Model.DateCreated</p>

                    </div>
                    <!-- /.col -->
                </div>
                <div class="row">
                    <!-- accepted payments column -->
                    <div class="col-xs-6">
                        <p class="lead">Tổng tiền:</p>
                    </div>
                    <!-- /.col -->
                    <div class="col-xs-6">
                        <p class="lead">@Model.Total</p>

                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
                <!-- this row will not appear when printing -->
                <div class="row no-print">
                    <div class="col-xs-12">
                        <a href="/admin/booking/view/@Model.Id" target="_blank" class="btn btn-default"><i class="fa fa-print"></i> Print</a>
                        @*<button type="button" class="btn btn-success pull-right">
                            <i class="fa fa-credit-card"></i> Submit Payment
                        </button>
                        <button type="button" class="btn btn-primary pull-right" style="margin-right: 5px;">
                            <i class="fa fa-download"></i> Generate PDF
                        </button>*@
                    </div>
                </div>
            </section>


           
        </div><!-- /.box-body -->
    </div><!-- /.box -->
</section><!-- /.content -->